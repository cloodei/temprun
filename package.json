{
  "name": "temprun",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun run build --compile --minify --target bun --outfile server ./src/index.ts",
    "build-js": "bun run build --compile --minify-whitespace --minify-syntax --target bun --outfile ./dist/index.js ./src/index.js",
    "prod": "./server",
    "prod-js": "bun run ./dist/index.js"
  },
  "dependencies": {
    "@elysiajs/cookie": "^0.8.0",
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/jwt": "^1.3.1",
    "@influxdata/influxdb3-client": "^1.2.0",
    "elysia": "latest",
    "mqtt": "^5.13.2"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "module": "src/index.js"
}
